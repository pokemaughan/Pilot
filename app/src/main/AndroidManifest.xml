<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kaylee.pilot">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/pilot_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity" />
        <activity android:name=".LogoPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EnterLocation" />
        <activity android:name=".CarSettings" />
        <activity android:name=".Airfare" />
        <activity android:name=".Hotel" />
        <activity android:name=".Settings" />
        <activity android:name=".SignIn" />
        <activity android:name=".Register" />
        <activity android:name=".FlyResults" />
        <activity android:name=".DriveResults" />
        <activity android:name=".Calculating"></activity>
    </application>

</manifest>